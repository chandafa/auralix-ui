{
  "name": "auralix-ui",
  "version": "1.0.1",
  "description": "Modern UI component library with glass morphism effects for React",
  "author": "Auralix UI Team",
  "license": "MIT",
  "keywords": [
    "react",
    "ui",
    "components",
    "tailwindcss",
    "glass-morphism",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chandafa/auralix-ui.git"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": ">=18.0.0 || ^19.0.0",
    "react-dom": ">=18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "rimraf": "^5.0.0"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "tailwind-merge": "^2.2.0"
  }
}
